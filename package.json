{
  "name": "@pinax/chain-metadata",
  "version": "1.0.0",
  "description": "Single-source-of-truth for the metadata of chains supported by Pinax.",
  "keywords": [
    "chains",
    "metadata",
    "ssot"
  ],
  "author": "Dominic <dominic@pinax.network>",
  "main": "index.js",
  "types": "./index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run generate && tsc",
    "generate": "npm run generate_chain_index && npm run generate_pinaxid_type && npm run generate_fill_chains_data && npm run format",
    "generate_chain_index": "node ./scripts/generate/chain_index.js",
    "generate_fill_chains_data": "node ./scripts/generate/fill_chains_data.js",
    "generate_pinaxid_type": "node ./scripts/generate/pinaxid_type.js",
    "fetch-graph-ids": "node ./scripts/fetchGraphIDs.js",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "wagmi": "^2.5.7"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinax-network/chains-metadata.git"
  },
  "homepage": "https://github.com/pinax-network/chains-metadata#readme",
  "bugs": {
    "url": "https://github.com/pinax-network/chains-metadata/issues"
  }
}
