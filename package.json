{
  "name": "@pinax/chain-metadata",
  "version": "1.0.0",
  "description": "Single-source-of-truth for the metadata of chains supported by Pinax.",
  "keywords": [
    "chains",
    "metadata",
    "ssot"
  ],
  "author": "Dominic <dominic@pinax.network>",
  "main": "index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "npm run generate && tsc",
    "fetch-graph-ids": "node ./scripts/fetchGraphIDs.js",
    "format": "prettier --log-level warn --write \"./**/*.{js,jsx,ts,tsx}\"",
    "generate": "npm run generate:pinaxid && npm run generate:graphid && npm run generate:index && npm run format",
    "generate:index": "node ./scripts/generate/chain_index.js",
    "generate:graphid": "node ./scripts/generate/graphid_type.js",
    "generate:pinaxid": "node ./scripts/generate/pinaxid_type.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky"
  },
  "dependencies": {
    "wagmi": "^2.5.7"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinax-network/chains-metadata.git"
  },
  "homepage": "https://github.com/pinax-network/chains-metadata#readme",
  "bugs": {
    "url": "https://github.com/pinax-network/chains-metadata/issues"
  }
}
